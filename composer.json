{
    "name": "kajuzi/laravel-paygate-payweb-3",
    "description": "PayGate's PayWeb3 integration for Laravel 10+",
    "keywords": [
        "Laravel",
        "paygate",
        "payweb3"
    ],
    "version": "0.0.2",
    "homepage": "https://gitlab.com/kajuzi_foss/laravel-paygate-payweb-3",
    "license": "MIT",
    "authors": [
        {
            "name": "Kajuzi Digital",
            "email": "dns@kajuzi.co.za"
        },
        {
            "name": "Peter Parsons",
            "email": "peterw.parsons@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-bcmath": "*",
        "guzzlehttp/guzzle": "^6.3|^7.0.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "PWParsons\\PayGate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PWParsons\\PayGate\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "PWParsons\\PayGate\\PayGateServiceProvider"
            ],
            "aliases": {
                "PayGate": "PWParsons\\PayGate\\PayGateFacade"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
